# Doxyfile for libwad
PROJECT_NAME = libwad
PROJECT_NUMBER = ${LIBWAD_VERSION}

RECURSIVE = YES
SHOW_NAMESPACES = YES

INPUT = ${CMAKE_CURRENT_SOURCE_DIR}/docs ${CMAKE_CURRENT_SOURCE_DIR}/src ${CMAKE_CURRENT_SOURCE_DIR}/include
